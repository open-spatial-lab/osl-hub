{
  "name": "sveltekit-nextauth",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run prebuild && vite dev",
    "build": "npm run prebuild && vite build",
    "preview": "npm run prebuild && vite preview",
    "start": "npm run prebuild && HOST=127.0.0.1 PORT=5173 ORIGIN=http://localhost:5173 node ./build",
    "check": "npm run prebuild && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm run prebuild && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "prebuild": "npm run fix-popper",
    "fix-popper": "node ./scripts/fixPopper.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0-next.80",
    "@sveltejs/adapter-node": "1.0.0-next.96",
    "@sveltejs/kit": "1.0.0-next.511",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "~4.8.2",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "@notion-stuff/blocks-html-parser": "^6.0.0",
    "@notion-stuff/blocks-markdown-parser": "^6.0.0",
    "@notionhq/client": "^2.2.2",
    "cookie": "0.5.0",
    "highlight.js": "^11.7.0",
    "keyv": "^4.5.2",
    "marked": "^4.2.3",
    "next-auth": "latest",
    "postcss": "^8.4.19",
    "postcss-load-config": "^4.0.1",
    "sveltestrap": "^5.10.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": false
  }
}
